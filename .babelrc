{
    "presets": ["es2015", "react", "stage-3"],
    "plugins": [
        "transform-runtime",
        "syntax-dynamic-import",
        "transform-class-properties", ["module-resolver",
        {
            "root": ["./src"],
            "alias":
            {
                'SRC': './',
                'LESS': 'assets/less',
                'IMAGES': 'assets/images',
                'SVGS': 'assets/svgs',
                'CONTAINER': 'container',
                'COMPONENTS': 'components',
                'CONSTANTS': 'constants',
                'SERVICE': 'service',
                'UTILS': 'utils',
                'REDUX': 'redux',
                'CONFIG': 'config'
            }
        }]
    ]

}
