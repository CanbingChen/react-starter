{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "react-starter",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dashboard -- babel-node ./build/dev.js",
    "clean": "rimraf dist coverage stats.json",
    "lint": "eslint -c ./.eslintrc.js --format node_modules/eslint-friendly-formatter --ignore-pattern *.spec.js --ignore-pattern 'node_modules/' src/ || exit 0",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test:coverage": "rimraf coverage && cross-env NODE_ENV=test jest --coverage || exit 0",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:karma": "rimraf test/unit/coverage/ && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "bundle:json": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config build/webpack.config.prod.js --json>stats.json",
    "bundle:dev": "cross-env NODE_ENV=development BABEL_ENV=production webpack --config build/webpack.config.prod.js",
    "bundle:test": "cross-env NODE_ENV=test BABEL_ENV=production webpack --config build/webpack.config.prod.js",
    "bundle:prod": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config build/webpack.config.prod.js",
    "build": "npm test && npm run clean && npm run bundle:dev",
    "serve": "serve -s ./dist",
    "prepush": "cross-env NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/CanbingChen/react-starter.git"
  },
  "keywords": [
    "react",
    "react-starter"
  ],
  "author": "ccb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CanbingChen/react-starter.git/issues"
  },
  "homepage": "https://github.com/CanbingChen/react-starter.git/blob/master/README.md",
  "jest": {
    "globals": {
      "CONFIG_ENV": true
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 60,
        "functions": 60,
        "lines": 60
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/build/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/build/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "\\.(local.less)$": "identity-obj-proxy",
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/build/jest/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)": "<rootDir>/build/jest/fileMock.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "src/.*\\.spec\\.js$"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^1.5.2",
    "jasmine-core": "^2.5.2",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "lint-staged": "^3.4.1",
    "moxios": "^0.4.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-partial-import": "^4.1.0",
    "postcss-utils": "^1.0.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-test-utils": "^15.5.1",
    "react-imageview": "^1.2.1",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^3.0.1",
    "rimraf": "^2.6.1",
    "serve": "^5.1.4",
    "sinon": "^3.3.0",
    "style-loader": "^0.16.1",
    "svg-fill-loader": "0.0.8",
    "svg-sprite-loader": "^3.2.5",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.4.2",
    "webpack-simple-progress-plugin": "^0.0.1"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "lodash.isequal": "^4.5.0",
    "normalize.css": "^7.0.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-partial-import": "^4.1.0",
    "postcss-utils": "^1.0.2",
    "prop-types": "^15.5.8",
    "query-string": "^5.0.0",
    "react": "^15.5.4",
    "react-css-modules": "^4.7.1",
    "react-dom": "^15.5.4",
    "react-modal": "^3.1.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-swipeable-views": "^0.12.8",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "store": "^2.0.4",
    "styled-components": "^1.4.6"
  },
  "lint-staged": {
    "*.js": "eslint -c ./.eslintrc.js --format node_modules/eslint-friendly-formatter --ignore-pattern *.spec.js --fix"
  }
}
